{
  "name": "@supra/pico",
  "description": "A simple javscript client side manager for @picocss/pico",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "module": "out/index.js",
  "types": "out/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./out/index.js",
      "types": "./out/lib/index.d.ts",
      "default": "./out/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --write lib",
    "lint": "eslint lib",
    "test": "DEBUG=supra:pico:tests:* bun run lib/__tests__/index.test.ts",
    "bundle:browser": "bun run build/browser.ts",
    "check-types": "tsc -p tsconfig.json",
    "build": "run-p bundle:*",
    "release": "release-it"
  },
  "packageManager": "bun@1.1.34",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@picocss/pico": "^2.0.6",
    "@supra/configs": "workspace:*",
    "@types/bun": "latest",
    "@types/debug": "^4.1.12",
    "debug": "^4.3.7",
    "esbuild": "^0.24.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "node-emoji": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0"
  }
}
