{
  "name": "@supra-dev/react",
  "description": "A library to support rendering React components in non-js server environments",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "module": "out/index.js",
  "types": "out/lib/index.d.ts",
  "files": ["out"],
  "exports": {
    ".": {
      "import": "./out/index.js",
      "types": "./out/lib/index.d.ts",
      "default": "./out/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --write lib",
    "lint": "eslint lib",
    "check-types": "tsc -p tsconfig.json",
    "bundle:cli": "bun run build/cli.ts",
    "bundle:cli:experimental": "bun run build/cli.experimental.ts",
    "build": "run-s check-types bundle:*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "packageManager": "bun@1.1.34",
  "peerDependencies": {
    "react": "^19.0.0-rc-5b0ef217-20241202",
    "react-dom": "^19.0.0-rc-5b0ef217-20241202",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@eslint/js": "^9.16.0",
    "@supra-dev/configs": "workspace:*",
    "@types/bun": "latest",
    "@types/debug": "^4.1.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "debug": "^4.3.7",
    "eslint": "^9.16.0",
    "globals": "^15.12.0",
    "node-emoji": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^19.0.0-rc-5b0ef217-20241202",
    "react-dom": "^19.0.0-rc-5b0ef217-20241202",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}
